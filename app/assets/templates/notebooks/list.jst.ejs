<header class="group">
    <div>
        <h2>Notebooks</h2>
    </div>
    <img class="dropdown-button" alt="*" src="data:image/png;base64,">
    <ul class="dropdown-menu hide">
        <li>
            <a href="#" class="new">New Notebook...</a>
        </li>
    </ul>
</header>
<ul class="notebook-detail">
    <li>
        <a class="group" href="#">
            <img class="icon" alt="*" src="data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAABAAAAAQCAYAAAAf8/9hAAAAm0lEQVR42mNgwALya5rVC2va5hTWtv0srGktZSAWFNS2WRbWtq4DavyPhJ/X19ez4ddY1+oNxIfRNCJwXVsyPlvPoGtgIAUAnTwTmwE4XYNuETjAatv+kG0A1BXrKDIAEvqYBuD3Opo8UOAA2S6ARSVFLoAKXiXbBSBQVNuaQJELQEkWKPGEbBeAXVHTlkmRAbCMQ7YByH4kxgAAl9BUWxvsjH0AAAAASUVORK5CYII=">
            <h3>
                All Notes
            <h3>
        </a>
    </li>
    <% Nevernote.notebooks.each(function(notebook) { %>
    <li class="group">
        <% var id = notebook.get('id'); %>
        <a class="group" href="#/notebooks/<%= id %>">
            <img class="icon" src="data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAABAAAAAQCAYAAAAf8/9hAAAAVUlEQVR42mNgGFSguL5Nq7C2bSkQ/ycCH8AwACh4tbCuPZAYy4BqH2IT/IBuE5o8XGzUgCFlQEFdq3dBbdsZUPogywBseHAYAPTWRmwGPCHCgM/IegAsqyv9CNAK4AAAAABJRU5ErkJggg==">
            <h3>
                <%= notebook.escape('name') %>
                (<%= notebook.get('note_count') %>)
            </h3>
        </a>
        <img class="dropdown-button" src="data:image/png;base64,">
        <ul class="dropdown-menu hide">
            <li>
                <a href="#" data-id="<%= id %>" class="rename">
                    Rename...
                </a>
            </li>
            <li>
                <a href="#" data-id="<%= id %>" class="properties">
                    Properties...
                </a>
            </li>
            <li>
                <a href="#" data-id="<%= id %>" class="delete">
                    Delete
                </a>
            </li>
        </ul>
    </li>
  <% }); %>
</ul>
